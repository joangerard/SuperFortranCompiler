BEGINPROG PrimeNumber
/* Returns the greater value of two numbers, if the values are the same it returns -1 */
VARIABLES x,y,result    /* Declare variables X and Y */
  READ(x)              // Read number a
  READ(y)              // Read a number from user input
  IF (x == y) THEN
    result:= -1 ELSE
    IF (x > y) THEN
    result := x ELSE
    IF (y > x) THEN
    result := y
    ENDIF
    ENDIF
    ENDIF
    PRINT(result)
ENDPROG