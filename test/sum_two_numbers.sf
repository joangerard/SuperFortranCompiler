BEGINPROG sUmtwOnUmBeRs
/* Compute the factorial of a number
   If the input number is negative, print -1. */
VARIABLES NUMBER, 0A123, result    // single comment
  READ(NUMBER)              // Read a number from user input
  READ(0A123)              // Read a number from user input
  result = NUMBER + 0A123
  result = 2
  result = 100
  result = 6
  PRINT(result)

ENDPROG